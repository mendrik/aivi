module examples.compiler.generators = {
  export gen, evens, grid, fibs

  gen = generate {
    yield 1
    yield 2
    yield 3
  }

  evens = generate {
    x <- [1..10]
    x -> _ % 2 == 0
    yield x
  }

  grid = generate {
    x <- [0..2]
    y <- [0..3]
    yield (x, y)
  }

  fibs = generate {
    loop (a, b) = (0, 1) => {
      yield a
      recurse (b, a + b)
    }
  }
}
