module repro_generators.main

export def main: Effect Unit = effect {
  _ <- print "--- Basic Yield ---"
  let gen1 = generate {
    yield 1
    yield 2
    yield 3
  }
  _ <- printGen gen1
}

printGen = g => effect {
  let f1 = acc => x => [x, ...acc]
  let asList = gen => gen f1[]
  let list = asList g
  _ <- print "{reverse list}"
}
