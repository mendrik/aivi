domain Units over Quantity = {
  (+) : Quantity -> Quantity -> Quantity
  (+) = a b => { value: a.value + b.value, unit: a.unit }

  (-) : Quantity -> Quantity -> Quantity
  (-) = a b => { value: a.value - b.value, unit: a.unit }

  (*) : Quantity -> Float -> Quantity
  (*) = q s => { value: q.value * s, unit: q.unit }

  (/) : Quantity -> Float -> Quantity
  (/) = q s => { value: q.value / s, unit: q.unit }
}
