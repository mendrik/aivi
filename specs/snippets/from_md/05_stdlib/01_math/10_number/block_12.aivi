domain Complex over Complex = {
  (+) : Complex -> Complex -> Complex
  (+) = a b => { re: a.re + b.re, im: a.im + b.im }

  (-) : Complex -> Complex -> Complex
  (-) = a b => { re: a.re - b.re, im: a.im - b.im }

  (*) : Complex -> Complex -> Complex
  (*) = a b => {
    re: a.re * b.re - a.im * b.im,
    im: a.re * b.im + a.im * b.re
  }

  (/) : Complex -> Float -> Complex
  (/) = z s => { re: z.re / s, im: z.im / s }
}
